@model NatisTracker.ViewModels.MasterViewModel

<!DOCTYPE html>
@{
    ViewBag.Title = "UserView";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<html lang="en">
<head>
    <meta charset="utf-8">
    <title>User View &middot; FindMy Natis</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <style type="text/css">
        table {
            font-family: arial, sans-serif;
            border-collapse: collapse;
            width: 100%;
        }

        td, th {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }

        tr:nth-child(even) {
            background-color: #dddddd;
        }

        .container {
            height: 100%;
            padding: 5px 5px 5px 5px;
            margin-left: 1%;
            margin-right: 1%;
            width: 98%;
        }
    </style></head>

<body>

    <ul class="nav nav-tabs nav-pills" data-tabs="tabs" id="myTab">
        <li class="active"><a data-toggle="tab" href="#Menu">Menu</a></li>
        <li><a data-toggle="tab" href="#ViewRequests">View Requests</a></li>
        <li><a data-toggle="tab" href="#Search">Search Natis</a></li>
        <li class="dropdown">
            <a class="dropdown-toggle" data-toggle="dropdown">View Department<span class="caret"></span></a>
            <ul class="dropdown-menu" data-tabs="tabs" id="mySubTab">
                <li><a data-toggle="tab" href="#Maturities">Maturities</a></li>
                <li><a data-toggle="tab" href="#Legal">Legal</a></li>
                <li><a data-toggle="tab" href="#Call_Centre">Call Centre</a></li>
                <li><a data-toggle="tab" href="#Licensing">Fines & Licensing</a></li>
                <li><a data-toggle="tab" href="#Operations">Operations</a></li>
                <li><a data-toggle="tab" href="#Driver">Driver</a></li>
                <li><a data-toggle="tab" href="#Remarketing">Remarketing</a></li>
                <li><a data-toggle="tab" href="#Origination">Contracts Originations</a></li>
            </ul>
        </li>
        <li class="dropdown">
            <a class="dropdown-toggle" data-toggle="dropdown">Send Natis<span class="caret"></span></a>
            <ul class="dropdown-menu" data-tabs="tabs" id="SendingOption">
                <li><a data-toggle="tab" href="#SendToDriver">To Driver</a></li>
                <li><a data-toggle="tab" href="#SendToCustomer">To Customer</a></li>
            </ul>
        </li>
        <li><a data-toggle="tab" href="#ReceiveNatis">Receive Natis</a></li>
    </ul>

    <div class="tab-content">

        <div id="Menu" class="tab-pane fade in active">
            <div class="container">
                <ul class="list-item">
                    <li class="list-group-item"> <a href="@Url.Action("RequestNatis", "Process")" class="btn btn-default btn-block">Send Request</a></li>
                    <li class="list-group-item"> <a href="@Url.Action("Scan_To_Collect", "Process")" class="btn btn-default btn-block">SCAN to Collect</a></li>
                </ul>

            </div>
        </div>

        <div id="ViewRequests" class="tab-pane fade in">
            <div class="container">
                <div class="table-responsive table table-striped">
                    <table id="RequestsTable" class="table">
                        <thead>
                            <tr>
                                <th>Request Status</th>
                                <th>Request Date</th>
                                <th>Contract Number</th>
                                <th>Reply Date</th>
                                <th>Responder Name</th>
                            </tr>
                        </thead>
                        @if (Model.RequestViewModel != null)
                        {
                            foreach (var Requests in Model.RequestViewModel)
                            {
                        <tbody>
                            <tr>
                                <td>@Requests.RequestStatus</td>
                                <td>@Requests.RequestDate </td>
                                <td>@Requests.ContractNo</td>
                                <td>@Requests.ReplyDate</td>
                                <td>@Requests.Responder</td>
                            </tr>
                        </tbody>
                            }
                        }
                    </table>
                </div>
            </div>
        </div>

        <div id="Search" class="tab-pane fade in">
            <div class="container">
                @using (Html.BeginForm("UserView", "Users", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    if (Model.natisDataViewModel != null)
                    {
                <div class="table-responsive table table-striped">
                    <table id="searchTable" class="table">
                        <thead class="thead-dark">
                            <tr>
                                <th>Date Scanned</th>
                                <th>Contract Numbers</th>
                                <th>VIN Number</th>
                                <th>Registration Number</th>
                                <th>Engine Number</th>
                                <th>Car Make</th>
                                <th>Series Number</th>
                                <th>Description</th>
                                <th>Registration Date</th>
                                <th>Owner name</th>
                                <th>Owner ID</th>
                                <th>Natis Location</th>
                                <th>Contract Status</th>
                                <th>Status Description</th>
                            </tr>
                        </thead>

                        <tbody>
                            @foreach (var item in Model.natisDataViewModel)
                                    {
                                        string contractNumber = "";
                                        string contractStatus = "";
                                        string statusDescription = "";

                                        for (int i = 0; i < Model.contractData.Count(); i++)
                                        {
                                            if (Model.contractData[i].VinNumber.Equals(item.VinNumber))
                                            {
                                                contractNumber = Model.contractData[i].ContractNumber;
                                                contractStatus = Model.contractData[i].ContractStatus;
                                                statusDescription = Model.contractData[i].StatusDescription;
                                            }

                                        }

                                    <tr>
                                        <td>@item.DateLoaded</td>
                                        <td>@contractNumber</td>
                                        <td>@item.VinNumber</td>
                                        <td>@item.RegistrationNumber</td>
                                        <td>@item.EngineNumber</td>
                                        <td>@item.CarMake</td>
                                        <td>@item.SeriesNumber</td>
                                        <td>@item.Description</td>
                                        <td>@item.RegistrationDate</td>
                                        <td>@item.OwnerName</td>
                                        <td>@item.OwnerIdentityNumber</td>
                                        <td>@item.NatisLocation</td>
                                        <td>@contractStatus</td>
                                        <td>@statusDescription</td>
                                    </tr>

                                    }
                        </tbody>

                        <tfoot class="thead-dark">
                            <tr>
                                <th>Date Scanned</th>
                                <th>Contract Numbers</th>
                                <th>VIN Number</th>
                                <th>Registration Number</th>
                                <th>Engine Number</th>
                                <th>Car Make</th>
                                <th>Series Number</th>
                                <th>Description</th>
                                <th>Registration Date</th>
                                <th>Owner name</th>
                                <th>Owner ID</th>
                                <th>Natis Location</th>
                                <th>Contract Status</th>
                                <th>Status Description</th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                    }
                }
            </div>
        </div>

