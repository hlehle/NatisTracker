        <div id="ReceiveNatis" class="tab-pane fade in">
            @using (Html.BeginForm("UserView", "Users", FormMethod.Post))
            {
                <fieldset>
                    <div class="table-responsive table table-striped">

                        <br /><br />
                        @if (Model._DriverPackage != null)
                        {
                            <table class="table-responsive">
                                <thead>
                                    <tr>
                                        <th>Sent By</th>
                                        <th>Sent Date</th>
                                        <th>Item Quantity</th>
                                        <th>Contract Numbers</th>
                                        <th>Accept</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @for (int i = 0; i < Model._DriverPackage.PackageItems.Count; i++)
                                    {
                                        var delivery = Model._DriverPackage.PackageItems[i];
                                        <tr>
                                            <td>@delivery.SentBy</td>
                                            <td>@delivery.SentDate</td>
                                            <td>@delivery.ItemQuantity</td>
                                            <td>
                                                @for (int j = 0; j < Model._DriverPackage.PackageItems[i].ContractNumbers.Count; j++)
                                                {
                                                    if (Model._DriverPackage.PackageItems[i].ContractNumbers[j].IsReceived)
                                                    {
                                                        @Html.HiddenFor(a => Model._DriverPackage.PackageItems[i].DriverId)
                                                        <div class="checkbox">
                                                            @Html.CheckBoxFor(a => Model._DriverPackage.PackageItems[i].ContractNumbers[j].IsReceived)
                                                            @Model._DriverPackage.PackageItems[i].ContractNumbers[j].ContractNumber<br />
                                                            @Html.HiddenFor(a => Model._DriverPackage.PackageItems[i].ContractNumbers[j].ContractNumber)
                                                            @Html.HiddenFor(a => Model._DriverPackage.PackageItems[i].ContractNumbers[j].DriverId)
                                                        </div>
                                                    }

                                                }
                                            </td>

                                            <td>
                                                @Html.DropDownListFor(a => a._DriverPackage.PackageItems[i].Confirmation, new List<SelectListItem>
                                                {
                                                    new SelectListItem { Text="Reply", Value = "Transit"},
                                                    new SelectListItem { Text="Accept", Value = "Accepted"},
                                                    new SelectListItem { Text="Deny", Value = "Denied"},
                                                })

                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>

                        }

                        <div class="checkbox">
                            @Html.CheckBoxFor(a => Model._DriverPackage.IsConfirmed)
                            @Html.HiddenFor(a => Model._DriverPackage.IsConfirmed)
                            @Html.HiddenFor(a => Model._DriverPackage.PackageId)

                            <p>I confirm that I have received the above checked natis documents</p>
                        </div>

                    </div>

                    @*<label class="checkbox">
                            <input type="checkbox" value=""> Remember me
                        </label>*@

                    <button class="btn btn-large btn-primary" type="submit">Send</button>
                </fieldset>
            }
        </div>

    </div>

    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script type="text/javascript">
            $(function () {
                $("#btnSubmit").click(function () {

                    var contracts = "";
                    var q = $("#Quantity").val();
                    for (var i = 1; i <= q; i++) {
                        debugger;
                        var con = prompt("Please enter Proposal Number " + i + " of " + q, "");
                        var length = con.length;
                        var IsNotNumber = isNaN(con);

                        //while invalid entry
                        while (length < 6 || IsNotNumber) {
                            alert("Proposal Number "+i+" of "+q+" is Incorrect, Please try again");
                            con = prompt("Please enter Proposal Number " + i + " of " + q, "");
                            length = con.length;
                            IsNotNumber = isNaN(con);
                        }
                        contracts = contracts + con + " ";
                    }

                    $("#@Html.IdFor(model => model.DriverView.ContractNumber)").val(contracts);

                });
            });
    </script>

